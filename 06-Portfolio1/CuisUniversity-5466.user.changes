!Track methodsFor: 'car position' stamp: 'MSS 9/21/2022 19:07:27' prior: 50551869!
put: aCar at: aPosition

	| ix sector accumulatedLength |
	
	aPosition > self length ifTrue: [ self error: self class cannotPlacedCarErrorDescription ].

	self remove: aCar.

	ix _ 1.
	sector _ sectors at: ix. 
	accumulatedLength _ 0 * kilometer.

	[aPosition > (accumulatedLength +  sector length)] whileTrue: [
		accumulatedLength _ accumulatedLength + sector length.
		ix _ ix + 1.
		sector _ sectors at: ix.
	].

	sector put: aCar at: aPosition - accumulatedLength.! !
!Track methodsFor: 'car position' stamp: 'MSS 9/21/2022 19:20:45' prior: 50560433!
put: aCar at: aPosition
	
	|remainingDistance|
	remainingDistance := aPosition.	
	aPosition > self length ifTrue: [ self error: self class cannotPlacedCarErrorDescription ].

	self remove: aCar.
	
	sectors do: [:sector | (remainingDistance > sector length) 
						ifTrue:[remainingDistance := remainingDistance - sector length]
						ifFalse:[sector put: aCar at: remainingDistance].
	].
"

	ix _ 1.
	sector _ sectors at: ix. 
	accumulatedLength _ 0 * kilometer.

	[aPosition > (accumulatedLength +  sector length)] whileTrue: [
		accumulatedLength _ accumulatedLength + sector length.
		ix _ ix + 1.
		sector _ sectors at: ix.
	].

	sector put: aCar at: aPosition - accumulatedLength.
	
"! !

