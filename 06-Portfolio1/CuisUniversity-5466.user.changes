put: aCar at: aPosition
	
	|remainingDistance|
	remainingDistance := aPosition.	
	self checkValidPosition: aPosition.
	self remove: aCar.
	sectors do: [:sector | ((self checkSector: sector isShorterThan: remainingDistance)) 
						ifTrue:[remainingDistance := remainingDistance - sector length]
						ifFalse:[sector put: aCar at: remainingDistance].
	].
	
